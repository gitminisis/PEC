<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers accessPolicy="Read, Execute, Script">
            <remove name="MIW" />
            <add name="mwimain" path="*.dll" verb="*" modules="IsapiModule" scriptProcessor="C:\inetpub\wwwroot\pec\scripts\MWIMain.dll" resourceType="File" preCondition="bitness32" />
            <add name="OAI_PMH" path="oai_pmh.dll" verb="*" modules="IsapiModule" scriptProcessor="C:\Inetpub\scripts\oai_pmh.dll" resourceType="Unspecified" preCondition="bitness32" allowPathInfo="true" />
        </handlers>
    </system.webServer>
</configuration>
